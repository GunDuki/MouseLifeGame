					if(singo==10){
						printf("욕을해서 정지먹었습니다. 멘탈 추가로 10감소 >_<\n");
						mental-=10;
					}
				}
				Sleep(1000);
				health-=2;
			}
			timer-=3;
			money-=3000;
			foodbar-=15;
			break;
		case 2://롤
			for(i=0;i<6;i++){
				srand(time(NULL));
				randomevent=rand()%2+1;
					srand(time(NULL));
