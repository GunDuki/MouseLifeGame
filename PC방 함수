						break;
					case 3:
						printf("피파에서 무승부를하였습니다. 멘탈 그대로\n");
				}
				Sleep(1000);
				health-=2;
			}
			timer-=2;
			money-=2000;
			foodbar-=10;
		}
		break;
	case 3:
		switch(gamechoice){
		case 1://서든
			for(i=0;i<6;i++){
				srand(time(NULL));
				randomevent=rand()%2+1;
				switch(randomevent){
				case 1://서든에서 이길경우
					printf("서든에서 이겼습니다. ^^ 멘탈 10증가\n");
					mental+=10;
