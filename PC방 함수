							printf("욕을해서 정지먹었습니다. 멘탈 추가로 10감소 >_<\n");
							mental-=10;
					}
				}
					health-=2;
					Sleep(1000);
			}
			timer-=2;
			money-=2000;
			foodbar-=10;
			break;
		case 3://피파
			for(i=0;i<4;i++){
				srand(time(NULL));
				randomevent=rand()%3+1;
				switch(randomevent){
					case 1:
