						printf("욕을해서 정지먹었습니다. 멘탈 추가로 10감소 >_<\n");
						mental-=10;
					}
				}
				health-=2;
				Sleep(1000);
			}
			timer-=2;
			money-=2000;
			foodbar-=10;
			break;
		case 2://롤
			for(i=0;i<4;i++){
				srand(time(NULL));
				randomevent=rand()%2+1;
					srand(time(NULL));
					randomevent=rand()%2+1;
					switch(randomevent){
					case 1:
						printf("롤에서 이겼습니다. ^^ 멘탈 15증가\n");
						mental+=15;
